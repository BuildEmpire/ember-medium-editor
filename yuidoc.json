{
  "name": "ember-medium-editor",
  "description": "medium-editor for Ember Apps",
  "version": "0.1.0",
  "url": "https://github.com/kolybasov/ember-medium-editor",
  "indexModule": "Usage",
  "options": {
    "paths": [
      "app",
      "addon"
    ],
    "themedir" : "node_modules/yuidoc-ember-theme",
    "helpers" : ["node_modules/yuidoc-ember-theme/helpers/helpers.js"],
    "exclude": "vendor",
    "outdir": "docs",
    "linkNatives": true,
    "quiet": true,
    "parseOnly": false,
    "lint": false,
    "nocode": false
  }
}
